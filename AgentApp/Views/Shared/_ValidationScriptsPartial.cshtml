<script>
    $(document).ready(function () {
        $('.sidebar-nav li').removeClass('active');

        // Highlight parent menu item.
        $('li a[href="' + this.location.pathname + '"]').parent().parent().addClass('in')
        $('ul a[href="' + this.location.pathname + '"]').parents('li').addClass('active')
        //  $('ul a[href="' + this.location.pathname + '"]').parents('li').find('span.arrow').addClass('open');
    })
    function showProcessing(btnId) {
        $("#" + btnId).hide();
        $("#showButtonLoading").show();
        $("#loading").show();
    }
    function hideProcessing(btnId, _IsValid, Msg, FormId="") {
        if (_IsValid) {
            toastr.success(Msg);
            $("#" + FormId).find('input').val('');
            $("#" + FormId).find('input:checkbox').prop('checked', false);
            $("#" + FormId).find('select').val('');
        }
        else
            toastr.error(Msg);
        $("#" + btnId).show();
        $("#showButtonLoading").hide();
        $("#loading").hide();
    }

</script>
